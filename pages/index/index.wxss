.swiper-1 swiper {
  width: 100wx;
  height: 340rpx;
}
.swiper-1 swiper image {
  width: 100%;
}
.daohang {
  display: flex;
}
.daohang navigator {
  flex: 1;
  padding: 20rpx;
}
.daohang navigator image {
  width: 100%;
}
.listgroup .loutitle {
  width: 100%;
  padding: 10rpx 0;
}
.listgroup .loutitle .louitem {
  overflow: hidden;
}
.listgroup .loutitle .louitem navigator {
  width: 33.33%;
  float: left;
  /* 2 3 两个超链接 */
}
.listgroup .loutitle .louitem navigator:nth-last-child(-n+4) {
  height: calc(33.33vw * 386 / 232 / 2);
  border-left: 10rpx solid #fff;
}
.listgroup .loutitle .louitem navigator:nth-child(2),
.listgroup .loutitle .louitem navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.listgroup .loutitle .louitem navigator image {
  width: 100%;
  height: 100%;
}
