<goodlisttabs tabs="{{tabs}}" bindtabsitem="tabsitem">
    <view class="group" wx:for="{{orders}}" wx:key="order_id">
        <view class="yi">
            <view class="yiright">订单价格</view>
            <view class="yileft">{{item.order_number}}</view>
        </view>
        <view class="er">
            <view class="erright">订单价格</view>
            <view class="erleft">￥{{item.order_price}}</view>
        </view>
        <view class="san">
            <view class="sanright">订单日期</view>
            <view class="sanleft">{{item.create_time_cn}}</view>
        </view>
    </view>
</goodlisttabs>
